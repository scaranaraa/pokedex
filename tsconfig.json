{
  "compilerOptions": {
    "target": "ES2022",                                   
    "module": "NodeNext",                                
    "outDir": "dist",
    "esModuleInterop": true,   
    "declaration": false,                            
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                   
    "inlineSourceMap": false,    
    "strictNullChecks": false,           
    "moduleResolution": "NodeNext",       
    "skipLibCheck": false ,                               
    "rootDir": "./src",
  }
}
