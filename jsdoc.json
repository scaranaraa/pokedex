{
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc"]
  },
  "source": {
      "include": ["dist/models.js","dist/pokemonbase.js","README.md"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
      "plugins/markdown"
  ],
  "opts": {
      "destination": "docs/",
      "encoding": "utf8",
      "recurse": true,
      "template": "./node_modules/clean-jsdoc-theme",
      "theme_opts": {
        "default_theme": "dark",
        "homepageTitle": "wp-bot",
        "includeFilesListInHomepage": false
    },
    "readme": "./README.md"
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  }
}

